[ignore]
<PROJECT_ROOT>/node_modules/config-chain/test/*

[include]

[libs]

[lints]
untyped-type-import=error

[options]
module.use_strict=true
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.ico
module.name_mapper='^inputFields\/\(.*\)$' ->'<PROJECT_ROOT>/src/modules/_shared/components/inputFields/\1'
module.name_mapper='^assets' ->'<PROJECT_ROOT>/src/assets'

[strict]
deprecated-type
sketchy-null
sketchy-number
unnecessary-optional-chain
unsafe-getters-setters
